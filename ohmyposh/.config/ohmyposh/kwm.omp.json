{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "palettes": {
        "template": "synthwave",
        "list": {
            "synthwave": {
                "COLOR_01": "#011627",
                "COLOR_02": "#fe4450",
                "COLOR_03": "#72f1b8",
                "COLOR_04": "#fede5d",
                "COLOR_05": "#03edf9",
                "COLOR_06": "#ff7edb",
                "COLOR_07": "#03edf9",
                "COLOR_08": "#ffffff",
                "COLOR_09": "#575656",
                "COLOR_10": "#fe4450",
                "COLOR_11": "#72f1b8",
                "COLOR_12": "#fede5d",
                "COLOR_13": "#03edf9",
                "COLOR_14": "#ff7edb",
                "COLOR_15": "#03edf9",
                "COLOR_16": "#ffffff",
                "BACKGROUND_COLOR": "#262335"
            },
            "gruvbox": {
                "COLOR_01": "#282828",
                "COLOR_02": "#cc241d",
                "COLOR_03": "#98971a",
                "COLOR_04": "#d79921",
                "COLOR_05": "#458588",
                "COLOR_06": "#b16286",
                "COLOR_07": "#689d6a",
                "COLOR_08": "#a89984",
                "COLOR_09": "#928374",
                "COLOR_10": "#fb4934",
                "COLOR_11": "#b8bb26",
                "COLOR_12": "#fabd2f",
                "COLOR_13": "#83a598",
                "COLOR_14": "#d3869b",
                "COLOR_15": "#8ec07c",
                "COLOR_16": "#ebdbb2",
                "BACKGROUND_COLOR": "#282828"
            }
        }
    },
    "blocks": [
        {
            "alignment": "left",
            "type": "prompt",
            "segments": [
                {
                    "foreground": "p:COLOR_04",
                    "properties": {
                        "root_icon": "\uF0E7"
                    },
                    "style": "plain",
                    "template": " \uF0E7",
                    "type": "root"
                },
                {
                    "type": "os",
                    "style": "plain",
                    "foreground": "p:COLOR_05",
                    "properties": {
                        "linux": "\uF17C",
                        "ubuntu": "\uF17C",
                        "debian": "\uF17C"
                    },
                    "template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}} "
                },
                {
                    "foreground": "p:COLOR_05",
                    "properties": {
                        "home_icon": "\uF015",
                        "folder_icon": "\uF07C",
                        "folder_separator_icon": " /",
                        "style": "agnoster"
                    },
                    "style": "plain",
                    "template": "{{ .Path }} ",
                    "type": "path"
                },
                {
                    "foreground": "p:COLOR_03",
                    "properties": {
                        "fetch_status": true,
                        "fetch_stash_count": true,
                        "fetch_upstream_icon": true,
                        "branch_icon": "\uE0A0 "
                    },
                    "style": "plain",
                    "template": "{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uF044 {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uF046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0}} \uF692 {{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}} \uf1bb {{ .WorktreeCount }}{{ end }} ",
                    "type": "git"
                },
                {
                    "foreground": "p:COLOR_04",
                    "properties": {
                        "style": "austin"
                    },
                    "style": "plain",
                    "template": " {{ .FormattedMs }} \uF252",
                    "type": "executiontime"
                }
            ]
        },
        {
            "alignment": "right",
            "type": "prompt",
            "segments": [
                {
                    "foreground": "p:COLOR_06",
                    "properties": {
                        "fetch_version": true
                    },
                    "style": "plain",
                    "template": " \uE718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
                    "type": "node"
                },
                {
                    "foreground": "p:COLOR_06",
                    "properties": {
                        "fetch_version": true,
                        "display_mode": "context"
                    },
                    "style": "plain",
                    "template": " \uE235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
                    "type": "python"
                },
                {
                    "foreground": "p:COLOR_06",
                    "properties": {
                        "fetch_version": true
                    },
                    "style": "plain",
                    "template": " \uFCD1 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
                    "type": "go"
                }
            ]
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "p:COLOR_16",
                    "style": "plain",
                    "template": "\u276F ",
                    "type": "text"
                }
            ],
            "type": "prompt"
        }
    ],
    "console_title_template": "{{if .Root}}root | {{end}} {{.Folder}}",
    "final_space": false,
    "version": 2
}
